<section class="cartPage">
  <div class="cartPageHead">
    <h1 class="cartPageTitle">Carrito</h1>
    <button class="pillBtn pillBtn--outline" id="cartPageClearBtn" type="button">Vaciar carrito</button>
  </div>

  <div class="cartPageGrid">
    <div class="cartPageList" id="cartPageList"></div>

    <aside class="cartSummary">
      <div class="cartSumRow">
        <span>Subtotal</span>
        <strong id="cartPageSubtotal">$0.00</strong>
      </div>

      <a class="pillBtn" href="/products">Seguir comprando</a>
      <div style="height:10px"></div>
      <button class="pillBtn pillBtn--outline" id="cartBuyBtn" type="button">
  Comprar
</button>
    </aside>
  </div>
</section>
